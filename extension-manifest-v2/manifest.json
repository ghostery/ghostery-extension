			"all_frames": true,
			"js": [
				"dist/content_script_bundle.js"
			],
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"exclude_matches": [
				"*://*/_/chrome/newtab*",
				"*://ntp.msn.com/*"
			],
			"run_at": "document_start"
		},
