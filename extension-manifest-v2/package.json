{
  "name": "@ghostery/extension-manifest-v2",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Ghostery GmbH",
    "email": "support@ghostery.com",
    "url": "https://www.ghostery.com"
  },
  "license": "MPL-2.0",
  "scripts": {
    "postinstall": "vendor-copy && npm run download-trackerdb",
    "start": "web-ext run --target=chromium",
    "build.dev": "webpack --mode development --progress",
    "build": "webpack --mode production --progress",
    "build.prod": "npm run build",
    "build.watch": "webpack --mode development --progress --watch",
    "licenses": "license-report --config=../.license-report-config.json > dist/licenses.html",
    "test": "jest",
    "test.watch": "jest --watch",
    "test.snapshot": "jest --updateSnapshot",
    "lint": "eslint --ext .js,.jsx ./app ./src ./test ./tools",
    "lint.fix": "eslint --ext .js,.jsx ./app ./src ./test ./tools --fix",
    "lint.raw": "eslint --ext .js,.jsx",
    "download-trackerdb": "node --input-type=module ./tools/download-trackerdb.js",
    "locales.ui": "hybrids extract -c --format=chrome.i18n ../node_modules/@ghostery/ui ./_locales/en/messages.json",
    "locales.src": "hybrids extract -c --format=chrome.i18n ./app ./_locales/en/messages.json",
    "locales": "npm run locales.ui && npm run locales.src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostery/ghostery-extension.git"
  },
  "bugs": {
    "url": "https://github.com/ghostery/ghostery-extension/issues",
    "email": "ghostery-dev@ghostery.com"
  },
  "homepage": "https://github.com/ghostery/ghostery-extension#readme",
  "dependencies": {
    "@cliqz/adblocker": "^1.26.12",
    "@cliqz/adblocker-webextension": "^1.26.12",
    "@cliqz/url-parser": "^1.1.5",
    "@duckduckgo/autoconsent": "^10.3.2",
    "@ghostery/libs": "^1.0.0",
    "@ghostery/trackers-preview": "^1.0.0",
    "@ghostery/ui": "^1.0.0",
    "@sentry/browser": "7.1.1",
    "@whotracksme/webextension-packages": "^5.0.3",
    "classnames": "^2.3.2",
    "d3": "^7.8.5",
    "foundation-sites": "^6.6.2",
    "ghostery-common": "^1.3.12",
    "history": "^4.10.1",
    "hybrids": "^8.2.5",
    "linkedom": "^0.14.21",
    "moment": "^2.29.1",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-svg": "^11.0.34",
    "react-window": "^1.8.8",
    "redux": "^4.0.5",
    "redux-object": "^0.5.10",
    "redux-thunk": "^2.4.2",
    "tldts-experimental": "^6.0.19",
    "underscore": "^1.13.6",
    "url": "^0.11.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^2.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "find-in-files": "^0.5.0",
    "fs-extra": "^9.0.1",
    "import-meta-loader": "^1.1.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-when": "^2.7.2",
    "jsonfile": "^6.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "oboe": "^2.1.5",
    "os-browserify": "^0.3.0",
    "path": "^0.12.7",
    "react-router": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.56.2",
    "sass-loader": "^10.4.1",
    "seamless-immutable": "^7.1.3",
    "sinon-chrome": "^3.0.1",
    "tty-browserify": "^0.0.1",
    "underscore-template-loader": "^1.2.0",
    "url-loader": "^4.1.1",
    "vendor-copy": "^3.0.1",
    "web-ext": "^7.11.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "vendorCopy": [
    {
      "from": "../node_modules/ghostery-common/build/gbe/assets",
      "to": "./common"
    }
  ]
}
