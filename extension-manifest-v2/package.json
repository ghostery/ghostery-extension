{
  "name": "@ghostery/extension-manifest-v2",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Ghostery GmbH",
    "email": "support@ghostery.com",
    "url": "https://www.ghostery.com"
  },
  "license": "MPL-2.0",
  "scripts": {
    "start": "web-ext run --target=chromium",
    "build.dev": "webpack --mode development --display-error-details --progress",
    "build.prod": "webpack --mode production --display-error-details --progress",
    "upload-sentry": "webpack --mode production --display-error-details --progress --devtool source-maps && ./tools/upload-sentry.sh",
    "build.watch": "webpack --mode development --display-error-details --progress --watch",
    "prebuild.dev": "node ./tools/licenses/fetchLicenses.js",
    "prebuild.prod": "node ./tools/licenses/fetchLicenses.js",
    "prebuild.watch": "node ./tools/licenses/fetchLicenses.js",
    "test": "cross-env BABEL_ENV=test jest",
    "test.watch": "cross-env BABEL_ENV=test jest --watch",
    "test.snapshot": "jest --updateSnapshot",
    "lint": "eslint --ext .js,.jsx ./app ./src ./test ./tools",
    "lint.fix": "eslint --ext .js,.jsx ./app ./src ./test ./tools --fix",
    "lint.raw": "eslint --ext .js,.jsx",
    "docs": "jsdoc -c jsdoc.json -d docs -r",
    "leet": "node ./tools/leet/leet-en.js",
    "leet.reset": "node ./tools/leet/leet-reset.js",
    "postinstall": "patch-package && vendor-copy",
    "locales.wtm": "hybrids extract -c --format=chrome.i18n node_modules/@whotracksme/webextension-packages/packages ./_locales/en/messages.json",
    "locales.ui": "hybrids extract -c --format=chrome.i18n node_modules/@ghostery/ui ./_locales/en/messages.json",
    "locales.src": "hybrids extract -c --format=chrome.i18n ./app ./_locales/en/messages.json",
    "locales": "npm run locales.wtm && npm run locales.ui && npm run locales.src"
  },
  "vendorCopy": [
    {
      "from": "node_modules/ghostery-common/build/gbe/assets",
      "to": "common"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostery/ghostery-extension.git"
  },
  "bugs": {
    "url": "https://github.com/ghostery/ghostery-extension/issues",
    "email": "ghostery-dev@ghostery.com"
  },
  "homepage": "https://github.com/ghostery/ghostery-extension#readme",
  "dependencies": {
    "@cliqz/url-parser": "^1.1.4",
    "@ghostery/libs": "file:../libs",
    "@ghostery/ui": "file:../ui",
    "@sentry/browser": "7.1.1",
    "@whotracksme/webextension-packages": "^0.4.2",
    "classnames": "^2.2.5",
    "d3": "^5.16.0",
    "foundation-sites": "^6.6.2",
    "ghostery-common": "^1.2.31",
    "history": "^4.10.1",
    "hybrids": "^8.1.1",
    "json-api-normalizer": "^1.0.4",
    "linkedom": "^0.14.5",
    "moment": "^2.29.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^7.1.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-svg": "^11.0.34",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-object": "^0.5.10",
    "redux-thunk": "^2.2.0",
    "rsvp": "^4.8.5",
    "spanan": "^2.0.0",
    "tldts-experimental": "^5.7.58",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.12.13",
    "@babel/preset-react": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "find-in-files": "^0.5.0",
    "fs-extra": "^9.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-when": "^2.7.2",
    "jsdoc": "^3.6.7",
    "jsonfile": "^6.1.0",
    "license-checker": "^25.0.1",
    "mini-css-extract-plugin": "^0.10.0",
    "oboe": "^2.1.5",
    "patch-package": "^6.4.7",
    "path": "^0.12.7",
    "react-router": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.54.0",
    "sass-loader": "^10.3.1",
    "seamless-immutable": "^7.1.3",
    "sinon-chrome": "^3.0.1",
    "svg-url-loader": "^6.0.0",
    "underscore-template-loader": "^1.1.0",
    "url-loader": "^4.1.1",
    "vendor-copy": "^3.0.1",
    "web-ext": "^7.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-shell-plugin": "^0.5.0"
  }
}
