{
  "name": "ghostery-extension",
  "private": true,
  "version": "10.5.22",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "start": "npm run build -- --watch",
    "start:update": "./scripts/update.sh",
    "debug": "npm run build -- --watch --debug",
    "lint": "npm run lint:src && npm run lint:build",
    "lint:src": "eslint",
    "lint:build": "npm run build firefox -- --silent && addons-linter dist",
    "test": "npm run wdio && npm run wdio:update",
    "wdio": "wdio tests/e2e/wdio.conf.js",
    "wdio:update": "wdio tests/e2e/wdio.update.conf.js",
    "wdio:page": "node tests/e2e/page/server.js",
    "locales:setup": "cp -f ./src/messages.json ./src/_locales/en/messages.json",
    "locales.src": "hybrids extract -c --format=chrome.i18n ./src ./src/_locales/en/messages.json",
    "locales": "npm run locales:setup && npm run locales.src",
    "xcode-export-locales": "rm -rf ./xcode/en.xcloc && xcodebuild -exportLocalizations -project ./xcode/Ghostery.xcodeproj -localizationPath ./xcode",
    "release": "./scripts/release.sh",
    "package": "./scripts/package.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/chrome": "^0.1.32",
    "@wdio/browser-runner": "^9.21.0",
    "@wdio/cli": "^9.21.1",
    "@wdio/globals": "^9.12.7",
    "@wdio/mocha-framework": "^9.12.6",
    "@wdio/spec-reporter": "^9.20.0",
    "addons-linter": "^9.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "license-report": "^6.8.1",
    "prettier": "^3.6.2",
    "sinon-chrome": "^3.0.1",
    "vite": "^7.2.6",
    "web-ext": "^9.2.0"
  },
  "dependencies": {
    "@duckduckgo/autoconsent": "^14.43.0",
    "@ghostery/adblocker": "^2.13.0",
    "@ghostery/adblocker-content": "^2.12.2",
    "@ghostery/adblocker-extended-selectors": "^2.12.2",
    "@ghostery/adblocker-webextension": "^2.13.0",
    "@ghostery/scriptlets": "github:ghostery/scriptlets",
    "@ghostery/urlfilter2dnr": "^1.6.3",
    "@github/relative-time-element": "^5.0.0",
    "@sentry/browser": "^10.30.0",
    "@whotracksme/reporting": "^7.10.0",
    "apexsankey": "github:ghostery/apexsankey",
    "bowser": "^2.13.1",
    "file-saver": "^2.0.5",
    "hybrids": "^9.1.21",
    "idb": "^8.0.2",
    "plotly.js-basic-dist": "^3.3.0",
    "tldts-experimental": "^7.0.19"
  },
  "dataDependencies": {
    "redirect-resources": "1c4e245353773758275d0b6a0ed1987aab16279b14d06780b6f2623266c0687f",
    "wtm-bloomfilter": "2025-12-01",
    "wtm-stats": "dc44480e5f348a60ea44a4618a530b72c13afe03"
  },
  "engineStrict": true,
  "engines": {
    "npm": ">=9.6.7"
  },
  "webExt": {
    "sourceDir": "./dist/"
  },
  "author": {
    "name": "Ghostery GmbH",
    "email": "info@ghostery.com",
    "url": "https://www.ghostery.com"
  },
  "license": "GPL-3.0"
}
