{
  "name": "ghostery-extension",
  "private": true,
  "version": "10.5.29",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "start": "npm run build -- --watch",
    "start:update": "./scripts/update.sh",
    "debug": "npm run build -- --watch --debug",
    "lint": "npm run lint:src && npm run lint:build",
    "lint:src": "eslint",
    "lint:build": "npm run build firefox -- --silent && addons-linter dist",
    "test": "npm run test:unit && npm run wdio && npm run wdio:update",
    "test:unit": "node --require ./tests/unit/setup/init-chrome.js --import ./tests/unit/setup/register-import.js --test",
    "wdio": "wdio tests/e2e/wdio.conf.js",
    "wdio:update": "wdio tests/e2e/wdio.update.conf.js",
    "wdio:page": "node tests/e2e/page/server.js",
    "locales:setup": "cp -f ./src/messages.json ./src/_locales/en/messages.json",
    "locales.src": "hybrids extract -c --format=chrome.i18n ./src ./src/_locales/en/messages.json",
    "locales": "npm run locales:setup && npm run locales.src",
    "xcode-export-locales": "rm -rf ./xcode/en.xcloc && xcodebuild -exportLocalizations -project ./xcode/Ghostery.xcodeproj -localizationPath ./xcode",
    "release": "./scripts/release.sh",
    "package": "./scripts/package.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/chrome": "^0.1.36",
    "@wdio/browser-runner": "^9.23.3",
    "@wdio/cli": "^9.24.0",
    "@wdio/globals": "^9.12.7",
    "@wdio/mocha-framework": "^9.12.6",
    "@wdio/spec-reporter": "^9.24.0",
    "addons-linter": "^9.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "license-report": "^6.8.1",
    "prettier": "^3.8.1",
    "sinon-chrome": "^3.0.1",
    "vite": "^7.3.1",
    "web-ext": "^9.2.0"
  },
  "dependencies": {
    "@duckduckgo/autoconsent": "^14.52.0",
    "@ghostery/adblocker": "^2.14.0",
    "@ghostery/adblocker-content": "^2.13.3",
    "@ghostery/adblocker-extended-selectors": "^2.13.3",
    "@ghostery/adblocker-webextension": "^2.14.0",
    "@ghostery/config": "^0.0.7",
    "@ghostery/scriptlets": "github:ghostery/scriptlets",
    "@ghostery/urlfilter2dnr": "^1.6.6",
    "@github/relative-time-element": "^5.0.0",
    "@sentry/browser": "^10.38.0",
    "@whotracksme/reporting": "^7.11.1",
    "apexsankey": "github:ghostery/apexsankey",
    "bowser": "^2.14.1",
    "csv-stringify": "^6.6.0",
    "file-saver": "^2.0.5",
    "hybrids": "^9.1.22",
    "idb": "^8.0.2",
    "lottie-web": "^5.13.0",
    "plotly.js-basic-dist": "^3.3.1",
    "tldts-experimental": "^7.0.22"
  },
  "dataDependencies": {
    "redirect-resources": "0a264a278c7277d0977ff85e5ebd4783c5c209f683d3f937f3da825388d335b3",
    "wtm-bloomfilter": "2026-02-02",
    "wtm-stats": "976071192e4c43919ca4d09e5c9a0b2a98431c6b"
  },
  "engineStrict": true,
  "engines": {
    "npm": ">=11.6.3"
  },
  "webExt": {
    "sourceDir": "./dist/"
  },
  "author": {
    "name": "Ghostery GmbH",
    "email": "info@ghostery.com",
    "url": "https://www.ghostery.com"
  },
  "license": "GPL-3.0"
}
