{
  "name": "ghostery-extension",
  "private": true,
  "version": "10.5.16",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "start": "npm run build -- --watch",
    "start:update": "./scripts/update.sh",
    "debug": "npm run build -- --watch --debug",
    "lint": "npm run lint:src && npm run lint:build",
    "lint:src": "eslint",
    "lint:build": "npm run build firefox -- --silent && addons-linter dist",
    "test": "npm run wdio && npm run wdio:update",
    "wdio": "wdio tests/e2e/wdio.conf.js",
    "wdio:update": "wdio tests/e2e/wdio.update.conf.js",
    "locales:setup": "cp -f ./src/messages.json ./src/_locales/en/messages.json",
    "locales.src": "hybrids extract -c --format=chrome.i18n ./src ./src/_locales/en/messages.json",
    "locales": "npm run locales:setup && npm run locales.src",
    "xcode-export-locales": "rm -rf ./xcode/en.xcloc && xcodebuild -exportLocalizations -project ./xcode/Ghostery.xcodeproj -localizationPath ./xcode",
    "release": "./scripts/release.sh",
    "package": "./scripts/package.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/chrome": "^0.1.24",
    "@wdio/browser-runner": "^9.20.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/globals": "^9.12.7",
    "@wdio/mocha-framework": "^9.12.6",
    "@wdio/spec-reporter": "^9.20.0",
    "addons-linter": "^8.4.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "license-report": "^6.8.1",
    "prettier": "^3.6.2",
    "sinon-chrome": "^3.0.1",
    "vite": "^7.1.12",
    "web-ext": "^9.1.0"
  },
  "dependencies": {
    "@duckduckgo/autoconsent": "^14.33.1",
    "@ghostery/adblocker": "^2.12.4",
    "@ghostery/adblocker-content": "^2.12.2",
    "@ghostery/adblocker-extended-selectors": "^2.12.2",
    "@ghostery/adblocker-webextension": "^2.12.4",
    "@ghostery/scriptlets": "github:ghostery/scriptlets",
    "@ghostery/urlfilter2dnr": "^1.6.3",
    "@github/relative-time-element": "^4.5.0",
    "@sentry/browser": "^10.22.0",
    "@whotracksme/reporting": "^7.8.0",
    "apexsankey": "github:ghostery/apexsankey",
    "bowser": "^2.12.1",
    "file-saver": "^2.0.5",
    "hybrids": "^9.1.20",
    "idb": "^8.0.2",
    "plotly.js-basic-dist": "^3.1.2",
    "tldts-experimental": "^7.0.17"
  },
  "dataDependencies": {
    "redirect-resources": "f4c6562d8f031655c19b05448021dc8597b76bad2e2799c63dafd6daf70173cc",
    "wtm-bloomfilter": "2025-10-20",
    "wtm-stats": "a8dd963cb6f854a67b04dd6d746b5416e7c537b9"
  },
  "engineStrict": true,
  "engines": {
    "npm": ">=9.6.7"
  },
  "webExt": {
    "sourceDir": "./dist/"
  },
  "author": {
    "name": "Ghostery GmbH",
    "email": "info@ghostery.com",
    "url": "https://www.ghostery.com"
  },
  "license": "GPL-3.0"
}
